# Required for memory features (semantic search via ChromaDB)
VOYAGE_API_KEY=your-voyage-api-key

# ChromaDB URL (optional - uses http://localhost:8000 if not set)
CHROMA_URL=http://localhost:8000

# Required for GitHub OAuth authentication
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# User identity (set to your GitHub username after OAuth login)
CALQ_USER=your-github-username

# HTTP server port (default: 3000)
MCP_PORT=3000

# Optional: Auth server port (default: 3847)
AUTH_PORT=3847

# Optional: OAuth callback URL (default: http://localhost:3847/callback)
OAUTH_CALLBACK_URL=http://localhost:3847/callback
